                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="7dp"
                        android:gravity="end"
                        android:text="주문번호"
                        android:textSize="15sp" />

                    <com.google.android.material.textfield.TextInputLayout
